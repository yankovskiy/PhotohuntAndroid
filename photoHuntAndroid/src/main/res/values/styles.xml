<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
<style name="AppBaseTheme" parent="Theme.AppCompat.Light">
    <item name="editTextColor">@color/material_blue_grey_700</item>
    <item name="colorPrimary">@color/material_blue_grey_500</item>
    <item name="colorPrimaryDark">@color/material_blue_grey_700</item>
    <item name="colorAccent">@color/material_teal_A500</item>
    <item name="android:textColorPrimary">@color/material_grey_50</item>
    <item name="android:textColor">@color/black54</item>
    <item name="android:colorBackground">@color/material_grey_50</item>
</style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>
